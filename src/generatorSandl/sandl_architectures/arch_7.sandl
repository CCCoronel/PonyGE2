neuralnet {
    input {
        features=10;
        sequence_length=30;
    }
    avg_pooling {
        dropout=0.1;
        l1=0.01;
        spectral_radius=0.9;
    }
    batch_norm {
        dilation_rate=2;
        leak_rate=0.3;
    }
    output {
        units=256;
        activation=softmax;
    }
}