neuralnet {
    input {
        features=50;
        sequence_length=30;
    }
    max_pooling {
        leak_rate=0.2;
        l2=0.0001;
        recurrent_dropout=0.2;
        heads=2;
        strides=1;
    }
    dense {
        recurrent_dropout=0.4;
        filters=16;
    }
    output {
        units=32;
        activation=leaky_relu;
        dropout=0.3;
    }
}