neuralnet {
    input {
        features=20;
        sequence_length=30;
    }
    tcn {
        leak_rate=0.3;
        units=256;
        heads=8;
        dilations=[4,8];
    }
    global_pooling {
        pool_size=3;
        kernel_init=random_normal;
        l2=0.01;
    }
    output {
        units=256;
        activation=sigmoid;
        dropout=0.4;
    }
}