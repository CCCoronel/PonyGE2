neuralnet {
    input {
        features=10;
        sequence_length=200;
    }
    global_pooling {
        dilation_rate=4;
        padding=same;
        strides=1;
    }
    gru {
        pool_size=2;
        heads=1;
        activation=relu;
        leak_rate=0.2;
        padding=causal;
    }
    output {
        units=64;
        activation=gelu;
    }
}