neuralnet {
    input {
        features=10;
        sequence_length=50;
    }
    batch_norm {
        epsilon=1e-5;
        padding=causal;
        units=128;
        activation=gelu;
    }
    avg_pooling {
        recurrent_dropout=0.5;
        l1=0.0001;
        input_scaling=1.5;
        spectral_radius=0.99;
    }
    output {
        units=256;
        activation=swish;
        dropout=0.4;
    }
}